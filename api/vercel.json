{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/",
            "methods": ["GET", "POST", "OPTIONS", "UPDATE", "PUT", "DELETE"]
        }
    ]
}
